{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"target": "es2017",
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": [
				"./*"
			]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"types/**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
